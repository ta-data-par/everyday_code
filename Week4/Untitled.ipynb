    "import random\n",
    "import itertools\n",
    " \n",
    "def get_random_pairs(numbers):\n",
    "  # Generate all possible non-repeating pairs\n",
    "  pairs = list(itertools.combinations(numbers, 2))\n",
    " \n",
    "  # Randomly shuffle these pairs\n",
    "  random.shuffle(pairs)\n",
    "  return pairs"
   ]
